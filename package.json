{
	"name": "minimal-mistakes",
	"version": "4.19.1",
	"description": "Minimal Mistakes 2 column Jekyll theme.",
	"repository": {
		"type": "git",
		"url": "git://github.com/mmistakes/minimal-mistakes.git"
	},
	"keywords": [
		"jekyll",
		"theme",
		"minimal"
	],
	"author": "Michael Rose",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/mmistakes/minimal-mistakes/issues"
	},
	"homepage": "https://mmistakes.github.io/minimal-mistakes/",
	"engines": {
		"node": ">= 0.10.0"
	},
	"devDependencies": {
		"npm-run-all": "^4.1.5",
		"onchange": "^7.0.2",
		"uglify-js": "^3.9.2"
	},
	"scripts": {
		"uglify": "uglifyjs assets/js/vendor/jquery/jquery-3.5.0.min.js assets/js/plugins/jquery.fitvids.js assets/js/plugins/jquery.greedy-navigation.js assets/js/plugins/jquery.magnific-popup.js assets/js/plugins/jquery.ba-throttle-debounce.js assets/js/plugins/gumshoe.js assets/js/_main.js -c -m -o assets/js/main.min.js",
		"add-banner": "node banner.js",
		"watch:js": "onchange \"assets/js/**/*.js\" -e \"assets/js/main.min.js\" -- npm run build:js",
		"build:js": "npm run uglify && npm run add-banner"
	}
}
